{
  "rootDir": "..",
  "verbose": true,
  "bail": true,
  "testRegex": "test/test-[a-z-]*\\.(js|jsx)$",
  "collectCoverage": false,
  "collectCoverageFrom": [
    "src/**"
  ],
  "coverageReporters": ["json", "text", "lcov"],
  "coverageDirectory": "coverage",
  "mapCoverage": true,
  "setupTestFrameworkScriptFile": "<rootDir>/test/_setup.js"
}
