{
  "plugins": [
    "syntax-object-rest-spread",
    "syntax-jsx",
    [
      "transform-object-rest-spread",
      {
        "useBuiltIns": true
      }
    ],
    [
      "transform-react-jsx",
      {
        "useBuiltIns": true
      }
    ],
    "transform-react-display-name",
    "external-helpers"
  ],
  "presets": [
    [
      "es2015",
      {
        "modules": false,
        "loose": true
      }
    ]
  ]
}
